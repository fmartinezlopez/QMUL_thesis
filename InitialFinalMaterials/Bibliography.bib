@Article{Wikstroem2009,
  author      = {G. Wikström and J. Edsjö},
  date        = {2009-03-17},
  title       = {{Limits on the WIMP-nucleon scattering cross-section from neutrino telescopes}},
  doi         = {10.1088/1475-7516/2009/04/009},
  eprint      = {0903.2986},
  eprintclass = {astro-ph.CO},
  eprinttype  = {arXiv},
  issn        = {1475-7516},
  number      = {04},
  pages       = {009--009},
  volume      = {2009},
  abstract    = {Neutrino-telescopes like Super-Kamiokande and IceCube have started to explore the neutrino fluxes from WIMP annihilations in the Sun. The non-observation of a signal can put constraints on the WIMP properties. We here focus on the neutrino signal from WIMP annihilation in the Sun and show that under reasonable assumptions, the non-observation of a signal from IceCube puts a much tighter constraint on the spin-dependent WIMP-proton scattering cross-section than current direct detection experiments like COUPP and KIMS. For the spin-independent scattering cross-section, the limits from IceCube and current direct detection experiments like XENON10 and CDMS place similar constraints. We here go through the assumptions being made and the uncertainties that arise in converting from limits on the muon flux from the Sun to limits on the WIMP-proton cross-section, and present our results as easy to use conversion factors.},
  comment     = {They comment on the WIMP capture rate systematics (solar model, form factor, local DM density and velocity distributions)},
  groups      = {SolarDM},
  journal     = {JCAP},
  keywords    = {astro-ph.CO},
  publisher   = {IOP Publishing},
}

